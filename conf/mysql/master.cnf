[mysqld]
skip_ssl
server-id = 1
binlog_format = row
gtid-mode = ON
enforce-gtid-consistency = true
log-slave-updates = true
log-bin = mysql-bin
log-bin-index = mysql-bin.index
log-error = mysql-bin.err
relay-log = relay-bin
relay-log-info-file = relay-bin.info
relay-log-index = relay-bin.index
expire_logs_days = 7
binlog-do-db = soc_net